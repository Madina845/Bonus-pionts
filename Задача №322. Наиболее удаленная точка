#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int n;
    cin >> n;  
    
    int x_max = 0, y_max = 0;  
    double max_distance = -1;  

    for (int i = 0; i < n; ++i) {
        int x, y;
        cin >> x >> y; 
        
        double distance = x * x + y * y;
        
        if (distance > max_distance) {
            max_distance = distance;
            x_max = x;
            y_max = y;
        }
    }

    cout << x_max << " " << y_max << endl;

    return 0;
}
