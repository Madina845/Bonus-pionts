#include <iostream>
#include <set>
using namespace std;

int main() {
    int N;
    cin >> N;  // Вводим количество чисел

    set<int> unique_numbers;  // Множество для хранения уникальных чисел

    for (int i = 0; i < N; ++i) {
        int x;
        cin >> x;  // Вводим число
        unique_numbers.insert(x);  // Добавляем число в множество
    }

    // Выводим размер множества, то есть количество уникальных чисел
    cout << unique_numbers.size() << endl;

    return 0;
}
