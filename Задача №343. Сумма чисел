#include <iostream>
using namespace std;

int main() {
    int N;
    long long sum = 0; // Переменная для хранения суммы
    int number;

    // Вводим количество чисел
    cin >> N;

    // Вводим числа и суммируем их
    for (int i = 0; i < N; ++i) {
        cin >> number;
        sum += number;
    }

    // Выводим сумму
    cout << sum << endl;

    return 0;
}
